}

/*公共控制模块*/
.defaultCommonDiv {
  background-color: rgba(23, 24, 23, 0.068);
  width: 200%;
  height: 7%;
  position:fixed;
  bottom:0px;
  display: flex;
}

/*跳转到主页*/
.mainPageBtn {
  margin-top: 50%;
  font-size:100%;
  margin-left: 60px;
  bottom: 0px;
  color:rgb(101, 101, 104);
  font-size:130%;
}

.myPageBtn {
  margin-bottom: 0px;
  font-size:100%;
  margin-left: 100px;
  color:rgb(101, 101, 104);
